<% @users.each do |user| %>
<div class="col-xs-3 col-md-2">
  

   <% if user.userface? %>
    <%= image_tag user.userface %>
    <% else %>
    <%= image_tag("userfacenoimage.png", :size => '90x90') %>

    <% end %><br>

    <div class="text-primary">
     <%= link_to user.username, user %>
     </div>


</div>
<% end %>
   
